                      <items>
                        <core:Item id="select_Item_paint1" key="paint1" text="{i18n>construct}" />
                        <core:Item id="select_Item_paint2" key="paint2" text="{i18n>car}" />
                        <core:Item id="select_Item_paint3" key="paint3" text="{i18n>boat}" />
                        <core:Item id="select_Item_paint4" key="paint4" text="{i18n>plane}" />
                      </items>
                    </Select>
