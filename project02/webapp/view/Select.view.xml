                  <FlexBox id="select_FlexBox_custId" direction="Row" alignItems="Center" class="displayFlex" justifyContent="SpaceBetween">
                    <Label id="select_Label_custId" text="{i18n>cust_id}" class="selectLabel" />
                    <Text id="select_Text_customerId" text="{/SelectedCustomer/CustomerID}" class="selectText" />
                  </FlexBox>

                  <!-- 기업명 -->
