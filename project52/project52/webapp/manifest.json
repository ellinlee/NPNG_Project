{
    "_version": "1.60.0",
    "sap.app": {
        "id": "project52",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.5",
            "toolsId": "2e9caf6b-14da-4408-ae78-8209df95b44f"
        },
        "dataSources": {
            "ZDCC_MAT_ID_FOR_BOM_CDS_VAN": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZDCC_MAT_ID_FOR_BOM_CDS_VAN',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/mainService/ZDCC_MAT_ID_FOR_BOM_CDS_VAN.xml"
                }
            },
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZDCC_MAT_ID_FOR_BOM_CDS/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "ZDCC_MAT_ID_FOR_BOM_CDS_VAN"
                    ],
                    "localUri": "localService/mainService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "project52.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "viewType": "XML",
                "async": true
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMain"
                    ]
                },
                {
                    "name": "RouteBOM",
                    "pattern": "BOM/{mat_id}/{mat_nm}",
                    "target": [
                        "TargetBOM"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "viewName": "Main",
                    "viewPath": "project52.view",
                    "viewLevel": 1
                },
                "TargetBOM": {
                    "viewName": "BOM",
                    "viewPath": "project52.view",
                    "viewLevel": 2
                }
            }
        },
        "rootView": {
            "viewName": "project52.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}